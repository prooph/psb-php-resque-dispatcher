{
    "name": "prooph/psb-php-resque-dispatcher",
    "description": "php-resque message dispatcher for PSB",
    "homepage": "https://github.com/prooph/psb-php-resque-dispatcher",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Alexander Miertsch",
            "email": "contact@prooph.de",
            "homepage": "http://www.codeliner.ws"
        }
    ],
    "keywords": [
        "prooph",
        "php",
        "cqrs",
        "messaging",
        "resque"
    ],
    "require": {
        "php": ">=5.5",
        "chrisboulton/php-resque": "~1.2"
    },
    "require-dev": {
        "prooph/service-bus": "dev-master",
        "phpunit/phpunit": "3.7.*"
    },
    "autoload": {
        "psr-4": {
            "Prooph\\ServiceBus\\Message\\PhpResque\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-0": {
            "Prooph\\ServiceBusTest\\": "vendor/prooph/service-bus/tests/"
        },
        "files" : [
            "tests/Mock/RemoveFileCommand.php",
            "tests/Mock/FileRemover.php"
        ]
    }
}
